{
  "name": "web-scraping-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "preview:dist": "vite preview --port 4173",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "docker:build": "docker build -t web-scraping-ui .",
    "docker:build:prod": "docker build -t web-scraping-ui:prod --build-arg VITE_API_BASE_URL=https://api.yourdomain.com .",
    "docker:run": "docker run -p 3000:80 web-scraping-ui",
    "ci:install": "npm ci --silent",
    "ci:build": "npm run type-check && npm run lint && npm run test:ci && npm run build:production && npm run build:check",
    "ci:test": "npm run type-check && npm run lint && npm run test:ci",
    "build:check": "node scripts/build-check.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-virtual": "^3.13.12",
    "@uiw/react-json-view": "^2.0.0-alpha.34",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.7.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.84.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^24.0.0",
    "msw": "^2.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.5",
    "vitest": "^1.3.1"
  }
}
